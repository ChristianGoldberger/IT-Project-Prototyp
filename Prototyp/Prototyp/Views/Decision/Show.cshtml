@{ 
    ViewBag.Title = "Entscheidungen";
}    


<fieldset style="margin-top: 10px">
    <legend>Entscheidungen</legend>
    <table id="decisionTable" class="table table-striped table-hover">
        <tbody>
            @foreach (var q in Decision)
            {
                <tr>
                    <td titel="@q.Titel">
                        @q.DecisionEntered
                    </td>
                    <td>
                        <a class="btn btn-primary" style="float:right;margin-left:5px; margin-bottom:2px; height:40px;width:80px" href="@Url.Action("Edit", new { controller = "Question", id = q.Key })">Ändern</a>
                        <a class="btn btn-danger" style="float:right;height:40px;width:80px" href="@Url.Action("Delete", new { controller = "Question", id = q.Key })">Löschen</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>

</fieldset>

@section scripts
{
    <script src="~/Scripts/question.js" type="text/javascript"></script>
    }








